@model Banking.Backup.SnapshotInfo
           
@using Banking.Web;

<tr data-entity="backup" data-view="view" data-id=@Model.Time.Ticks>
    <td>
        <input type="checkbox" class="itemCheck" data-action="select"/>
    </td>
    <td>
        <span data-action="edit" title="Edit backup «@Model.GetTitle()»" data-sort-key="time" data-sort-value=@Model.Time.Ticks>
            @Model.Time.ToString(Helper.BackupTimeFormat)
        </span>
    </td>
    <td><span data-action="edit" title="Edit backup «@Model.GetTitle()»" data-sort-key="mark">@Model.Mark</span></td>
    <td><span data-action="edit" title="Edit backup «@Model.GetTitle()»" data-sort-key="operations">@Model.OperationsCount</span></td>
    <td><span data-action="edit" title="Edit backup «@Model.GetTitle()»" data-sort-key="persons">@Model.PersonsCount</span></td>
</tr>